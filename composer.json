{
    "name": "catalytic/sdk",
    "type": "library",
    "description": "Catalytic's SDK",
    "keywords": [
        "Catalytic",
        "SDK",
        "Pushbot",
        "Automation"
    ],
    "homepage": "https://github.com/catalyticlabs/catalytic-sdk-php",
    "license": "proprietary",
    "authors": [
        {
            "name": "Catalytic, Inc",
            "homepage": "https://catalytic.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.16"
    },
    "autoload": {
        "psr-4": {
            "Catalytic\\SDK\\" : "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Catalytic\\Client\\" : "test/" }
    }
}
